{
    "env": {
        "node": true,
        "commonjs": true,
        "es2021": true,
        "jest/globals": true
    },
    "extends": [
        "prettier",
        "airbnb-base"
    ],
    "plugins": [
        "prettier",
        "jest"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "camelcase": [
            0,
            {
                "ignoreImports": false
            }
        ],
        "prettier/prettier": "error",
        "no-underscore-dangle": "off",
        "object-curly-newline": "off"
    }
}